{
  "name": "myApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "asset:install": "npx react-native-asset",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "postinstall": "jetify",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace bizlerajanda.xcworkspace -scheme bizlerajanda -configuration Release -archivePath bizlerajanda.xcarchive archive",
    "build": "npm run build:android && npm run build:ios",
    "release:android": "cd android && ./gradlew bundleRelease",
    "release:ios": "cd ios && xcodebuild -exportArchive -archivePath bizlerajanda.xcarchive -exportOptionsPlist exportOptions.plist -exportPath .",
    "release": "npm run release:android && npm run release:ios",
    "clean": "cd android && ./gradlew clean && cd .. && rm -rf ios/build",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "rm -rf ios/build",
    "clean:node_modules": "rm -rf node_modules",
    "clean:all": "npm run clean && npm run clean:node_modules",
    "pod:install": "cd ios && pod install && cd ..",
    "pod:update": "cd ios && pod update && cd ..",
    "pod:clean": "cd ios && rm -rf Pods && rm -rf Podfile.lock && cd ..",
    "husky:install": "husky install",
    "prepare": "npm run pod:install && npm run husky:install"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.0",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-native-firebase/app": "^20.1.0",
    "@react-native-firebase/crashlytics": "^20.5.0",
    "@react-native-firebase/messaging": "^20.1.0",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/elements": "^1.3.30",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "axios": "^1.7.2",
    "chess.js": "^1.4.0",
    "eslint-plugin-import": "^2.29.1",
    "i18n-js": "^4.4.3",
    "moment": "^2.30.1",
    "momentjs": "^2.0.0",
    "prop-types": "^15.8.1",
    "react": "19.1.1",
    "react-hook-form": "^7.51.5",
    "react-native": "0.82.1",
    "react-native-blob-util": "^0.19.9",
    "react-native-camera": "^4.2.1",
    "react-native-config": "^1.5.3",
    "react-native-document-scanner-plugin": "^1.0.1",
    "react-native-element-dropdown": "^2.12.1",
    "react-native-fs": "^2.15.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-image-crop-picker": "^0.41.1",
    "react-native-image-picker": "^7.1.2",
    "react-native-keychain": "^10.0.0",
    "react-native-localize": "^3.1.0",
    "react-native-modal": "^13.0.1",
    "react-native-pdf": "^6.7.5",
    "react-native-permissions": "^4.1.5",
    "react-native-qrcode-svg": "^6.3.1",
    "react-native-reanimated": "^3.15.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^3.31.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.1.1",
    "react-native-swiper": "^1.6.0",
    "react-native-version-check": "^3.4.7",
    "react-native-webview": "^13.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "jetifier": "^2.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
